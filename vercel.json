{
  "version": 2,
  "builds": [
    {
      "src": "api/create-order.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/create-order.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
