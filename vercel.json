{
  "version": 2,
  "builds": [
    {
      "src": "api/create-order.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/create-order",
      "methods": ["POST"],
      "dest": "/api/create-order.js"
    },
    {
      "src": "/api/health",
      "methods": ["GET"],
      "dest": "/api/create-order.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
