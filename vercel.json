{
  "version": {
    "required": 2
  },
  "builds": [
    "src": {
      "command": "next build",
      "dest": "build"
    }
  ],
  "routes": [
    "src": "/_next/*",
    "dest": "/_build",
    "headers": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  },
  {
    "src": "/api/.*",
    "dest": "build"
  },
  {
    "src": "/.*",
    "dest": "build"
  }
]
